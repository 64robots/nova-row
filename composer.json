{
  "name": "64robots/nova-row",
  "description": "A Laravel Nova row field.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "64robots/nova-fields": "^0.1.8"
  },
  "autoload": {
    "psr-4": {
      "R64\\NovaRow\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "R64\\NovaRow\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
